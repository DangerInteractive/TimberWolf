# - Config file for the TimberWolf package
# It defines the following variables
#  TimberWolf_INCLUDE_DIRS - include directories for TimberWolf
#  TimberWolf_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(TimberWolf_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(TimberWolf_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Library dependencies (contains definitions for imported targets)
include("${TimberWolf_CMAKE_DIR}/TimberWolfTargets.cmake")

# These are imported targets created by TimberWolfTargets.cmake
set(TimberWolf_LIBRARIES TimberWolf)
